<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/setting_device_content"
    >
    <include layout="@layout/menu_title_layout"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        android:layout_weight="1"
        android:background="@color/white_f8f8f8"
        >
        <View style="@style/divider_line1dp"/>
        <RelativeLayout
            android:id="@+id/setting_device_layout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:background="@color/white_ffffff"
            >
            <ImageView
                android:id="@+id/setting_device_img"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/print_device_icon"
                android:layout_centerVertical="true"
                />
            <TextView
                style="@style/setting_menu_tvTitleStyle"
                android:text="@string/setting_print_device"
                android:layout_toRightOf="@id/setting_device_img"
                />
            <TextView
                android:id="@+id/setting_device_tvDeviceState"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/setting_no_print_device"
                android:textSize="@dimen/TextSize16"
                android:textColor="@color/grey_666666"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                />
        </RelativeLayout>
        <View style="@style/divider_line1dp"/>
        <RelativeLayout
            android:id="@+id/setting_passwd_layout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:background="@color/white_ffffff"
            >
            <ImageView
                android:id="@+id/setting_passwd_img"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/settings_passwd_icon"
                android:layout_centerVertical="true"
                />
            <TextView
                style="@style/setting_menu_tvTitleStyle"
                android:text="@string/setting_passwd"
                android:layout_toRightOf="@id/setting_passwd_img"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/next_icon"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                />
        </RelativeLayout>
        <View style="@style/divider_line1dp"/>
        <RelativeLayout
            android:id="@+id/setting_permissions_layout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:background="@color/white_ffffff"
            >
            <ImageView
                android:id="@+id/setting_permissions_img"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/permissions_icon"
                android:layout_centerVertical="true"
                />
            <TextView
                style="@style/setting_menu_tvTitleStyle"
                android:text="@string/setting_permissions"
                android:layout_toRightOf="@id/setting_permissions_img"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/next_icon"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                />
        </RelativeLayout>
        <View style="@style/divider_line1dp"/>
        <View
            style="@style/divider_line1dp"
            android:layout_marginTop="10dp"
            />
        <RelativeLayout
            android:id="@+id/setting_system_layout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:background="@color/white_ffffff"
            >
            <ImageView
                android:id="@+id/setting_system_img"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/settings_icon"
                android:layout_centerVertical="true"
                />
            <TextView
                style="@style/setting_menu_tvTitleStyle"
                android:text="@string/setting_system"
                android:layout_toRightOf="@id/setting_system_img"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/next_icon"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                />
        </RelativeLayout>
        <View style="@style/divider_line1dp"/>
        <RelativeLayout
            android:id="@+id/setting_about_us_layout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:background="@color/white_ffffff"
            >
            <ImageView
                android:id="@+id/setting_img4"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/about_us_icon"
                android:layout_centerVertical="true"
                />
            <TextView
                style="@style/setting_menu_tvTitleStyle"
                android:text="@string/setting_about_us"
                android:layout_toRightOf="@id/setting_img4"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/next_icon"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                />
        </RelativeLayout>
        <View style="@style/divider_line1dp"/>
        <View
            style="@style/divider_line1dp"
            android:layout_marginTop="10dp"
            />
        <RelativeLayout
            android:id="@+id/setting_exit_layout"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:background="@color/white_ffffff"
            >
            <ImageView
                android:id="@+id/setting_exitImg"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/exit_icon"
                android:layout_centerVertical="true"
                />
            <TextView
                style="@style/setting_menu_tvTitleStyle"
                android:text="@string/setting_exit"
                android:layout_toRightOf="@id/setting_exitImg"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/next_icon"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
