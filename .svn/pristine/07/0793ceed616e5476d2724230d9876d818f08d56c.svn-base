	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("FacePayUnifiedOrderReq [mch_no=");
		builder.append(mch_no);
		builder.append(", term_no=");
		builder.append(term_no);
		builder.append(", nonce_str=");
		builder.append(nonce_str);
		builder.append(", sign=");
		builder.append(sign);
		builder.append(", sign_type=");
		builder.append(sign_type);
		builder.append(", term_transaction_sn=");
		builder.append(term_transaction_sn);
		builder.append(", face_code_type=");
		builder.append(face_code_type);
		builder.append(", total_fee=");
		builder.append(total_fee);
		builder.append(", openid=");
		builder.append(openid);
		builder.append(", pay_type=");
		builder.append(pay_type);
		builder.append(", face_code=");
		builder.append(face_code);
		builder.append(", telephone=");
		builder.append(telephone);
		builder.append(", body=");
		builder.append(body);
		builder.append(", goods_tag=");
		builder.append(goods_tag);
		builder.append(", detail=");
		builder.append(detail);
		builder.append(", attach=");
		builder.append(attach);
		builder.append("]");
		return builder.toString();
	}

	public String getPay_type() {
		return pay_type;
	}

	public void setPay_type(String pay_type) {
		this.pay_type = pay_type;
	}

	public String getOut_trade_no() {
		return out_trade_no;
	}

	public void setOut_trade_no(String out_trade_no) {
		this.out_trade_no = out_trade_no;
	}

